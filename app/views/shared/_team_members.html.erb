<div class="row my-1">
  <% active_members(team).each do |member| %>
    <div class="col-md-6 border border-right">
      <div class="row">
        <div class="col-md-2 m-1">
          <%= user_gravatar(member[1],58) %>
        </div>
        <div class="col-md-9 mx-1">
          <h6><strong><%=member[0] %></h6></strong>
            <h6><%=member[1] %></h6>
            <% created_at ='' %>
            <%  if member[2] %>
              <% created_at = member[3] || member[5]  %>
            <% else %>
              <% created_at = member[4] || member[3] %>
          <% end %>
          <h6><%= created_at.strftime("%b %d, %Y") %> </h6>
        </div>
      </div>
    </div>
  <% end %>
</div>
